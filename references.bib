@article{Scherzinger:2015,
title = "Tagging fast neutrons from an 241Am/9Be source",
journal = "Applied Radiation and Isotopes",
volume = "98",
pages = "74 - 79",
year = "2015",
issn = "0969-8043",
doi = "https://doi.org/10.1016/j.apradiso.2015.01.003",
url = "http://www.sciencedirect.com/science/article/pii/S0969804315000044",
author = "J. Scherzinger and J.R.M. Annand and G. Davatz and K.G. Fissum and U. Gendotti and R. Hall-Wilton and E. Håkansson and R. Jebali and K. Kanaki and M. Lundin and B. Nilsson and A. Rosborge and H. Svensson",
keywords = "Americium–beryllium, Gamma-rays, Fast neutrons, Time-of-flight",
abstract = "Shielding, coincidence, and time-of-flight measurement techniques are employed to tag fast neutrons emitted from an 241Am/9Be source resulting in a continuous polychromatic energy-tagged beam of neutrons with energies up to 7MeV. The measured energy structure of the beam agrees qualitatively with both previous measurements and theoretical calculations."
}

